<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane fx:id="menuLayout" xmlns:fx="http://javafx.com/fxml"
            fx:controller="GameManager.MenuController">
    <ImageView fx:id="menuBackground">
        <image>
            <Image url="@../res/menuBackground.png"/>
        </image>
    </ImageView>

    <AnchorPane fx:id="menuLayout_Button">
        <ImageView fx:id="btnPlayGame">
            <image>
                <Image url="@../res/menuPlayGame.png"/>
            </image>
        </ImageView>

        <ImageView fx:id="btnHighScores">
            <image>
                <Image url="@../res/menuBtnHighScores.png"/>
            </image>
        </ImageView>

        <ImageView fx:id="btnSetting">
            <image>
                <Image url="@../res/menuBtnSetting.png"/>
            </image>
        </ImageView>

        <ImageView fx:id="btnExit">
            <image>
                <Image url="@../res/menuBtnExit.png"/>
            </image>
        </ImageView>
    </AnchorPane>





</AnchorPane>


